{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ZipkinOptions": {
    "ApplicationName": "identity",
    "ZipkinCollectorUrl": "http://zipkin:9411"
  },
  "ServiceDiscovery": {
    "Service": {
      "Name": "identity",
      "Address": "identity",
      "Port": 5200,
      "Interval": 10
    },
    "UserServiceName": "identity",
    "Consul": {
      "HttpEndpoint": "http://consul-agent-1:8500"
    }
  }
}
